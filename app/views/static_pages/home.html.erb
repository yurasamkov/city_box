<h1>All post</h1>
<% if user_signed_in? %>
  <%= link_to 'New post', new_post_path, class: 'btn btn-success' %>
<%end%>  
</br> 
</br> 
</br>
</br>  
<table class="table">
  <thead>
    <tr>
      <th>ID</th>
      <th>Content</th>
      <th>User</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <th></th>
        <td><%= post.content %></td>
        <td><%= post.user_id %></td>
        <td><%= link_to 'Show', post %></td>

      </tr>
    <% end %>
  </tbody>
</table>


